  <body ng-app="helpmyusers" ng-controller="guidedTours">
 
  <div>
    <div class = "container-fluid">
    <div class="row">
      <div class="col-md-10">
        <div class = "row">
                <div class="col-md-3">
                  <label>Enter website URL:</label>
                </div>
                <div class="col-md-6">
                  <input type="text" ng-model="url">
                </div>
                <div class="col-md-3">
                  <button class="btn btn-primary" ng-click="loadURL();">GO</button>      
                  <button class="btn btn-primary" ng-click="publish();">Publish</button>    
                  
                </div>
      </div>
      
      
      <div class = "row imageClass" style = "margin-top:100px;">
        
      <img src="{{imagePath}}" height="100%" width="100%" ng-click="setMessage()">
      
    </div>
      </div>
      <div class="col-md-2">
      <div ng-repeat="tours in guidedTourData track by $index">
        <table class="table table-striped table-bordered" >
          <tr>
            <td rowspan="3">{{$index+1}}</td>
            <td>{{tours.id}}</td>
          </tr>
          <tr>
            <td>{{tours.header}}</td>
          </tr>
          <tr>
            <td>{{tours.message}}</td>
          </tr>

        </table>
        </div>
      </div>
    </div>
      
  </div>  
  </div>
 

<script type="text/javascript" src="../bower_components/angular-bootstrap/ui-bootstrap.min.js"></script>
