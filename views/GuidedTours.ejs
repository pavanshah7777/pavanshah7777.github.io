          .carousel-inner > .item > img,
          .carousel-inner > .item > a > img {
             width: 25%;
             margin: auto;
          }

          .imageClass {
          cursor:url(http://www.rw-designer.com/cursor-extern.php?id=12779), auto;
        }
        </style>
    
    <script type="text/javascript">
        $(document).ready(function() {
        $('.carousel').carousel({
          interval: 1500
        })
        });
    </script>
   
